	File = {:Users/cuklinaj/Dropbox/PhD/Articles{\_}sorted//Barrios et al. - 1998 - In vivo genomic footprinting analysis reveals that the complex Bradyrhizobium japonicum fixRnifA promoter region.pdf:pdf},
	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial: chemistry,Bacterial: drug effects,Bacterial: metabolism,Base Sequence,Chromosome Mapping,DNA,DNA Footprinting,DNA Methylation,DNA-Directed RNA Polymerases,DNA-Directed RNA Polymerases: metabolism,Genetic,Genetic: drug effects,Isomerism,Molecular Sequence Data,Mutagenesis,Oxidoreductases,Potassium Permanganate,Potassium Permanganate: pharmacology,Promoter Regions,Rhizobiaceae,Rhizobiaceae: genetics,Sulfuric Acid Esters,Sulfuric Acid Esters: pharmacology,Transcription Factors,Transcription Factors: genetics},
	Month = {feb},
