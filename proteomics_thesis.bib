	Keywords = {Antisense,Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Bacterial: genetics,Bacterial: metabolism,Bacterial: physiology,Base Composition,DNA,Gene Expression Profiling,Gene Expression Regulation,Genetic,Genome,Lab-On-A-Chip Devices,Membrane Proteins,Membrane Proteins: genetics,Membrane Proteins: metabolism,Mycobacterium tuberculosis,Mycobacterium tuberculosis: genetics,Mycobacterium tuberculosis: metabolism,Polymerase Chain Reaction,Polymerase Chain Reaction: methods,Promoter Regions,Protein Array Analysis,RNA,Sigma Factor,Sigma Factor: genetics,Sigma Factor: metabolism,Untranslated,Untranslated: metabolism},
