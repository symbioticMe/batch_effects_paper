	Doi = {10.1128/JB.01018-08},
	File = {:Users/cuklinaj/Dropbox/PhD/Articles{\_}sorted/Hodges et al. - 2009 - The Agrobacterium rhizogenes GALLS gene encodes two secreted proteins required for genetic transformation of plan.pdf:pdf},
