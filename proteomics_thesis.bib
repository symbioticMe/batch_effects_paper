	Issn = {1097-4644 (Electronic) 0730-2312 (Linking)},
	Journal = {J Cell Biochem},
	Keywords = {cell cycle,human hepatocellular carcinoma,no-tumor hepatitis/cirrhosis,survivin (BIRC5) computational network},
	Number = {5},
	Pages = {1286--1294},
	Pmid = {21312234},
	Title = {{Survivin (BIRC5) cell cycle computational network in human no-tumor hepatitis/cirrhosis and hepatocellular carcinoma transformation}},
	Type = {Journal Article},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/21312234},
