	Keywords = {Bacterial,Bacterial Proteins,Bacterial Proteins: metabolism,Culture Media,Gene Expression Regulation,Homeodomain Proteins,Homeodomain Proteins: metabolism,Hydrogen,Hydrogen: metabolism,Hydrogenase,Hydrogenase: genetics,Hydrogenase: metabolism,Malates,Molecular Sequence Data,Multigene Family,Nitrogen Fixation,Protein Kinases,Protein Kinases: metabolism,Repressor Proteins,Repressor Proteins: metabolism,Rhodopseudomonas,Rhodopseudomonas: genetics,Rhodopseudomonas: growth {\&} development,Rhodopseudomonas: metabolism},
