	Pmid = {23649834},
	Title = {{Identification of the nature of reading frame transitions observed in prokaryotic genomes.}},
	Type = {Journal Article},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3711429{\&}tool=pmcentrez{\&}rendertype=abstract http://www.ncbi.nlm.nih.gov/pubmed/23649834},
