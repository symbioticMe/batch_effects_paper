	Title = {{Genome analyses of three strains of Rhodobacter sphaeroides: evidence of rapid evolution of chromosome II.}},
	Type = {Journal Article},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1855717{\&}tool=pmcentrez{\&}rendertype=abstract http://jb.asm.org/cgi/doi/10.1128/JB.01498-06 http://www.ncbi.nlm.nih.gov/pubmed/17172323},
