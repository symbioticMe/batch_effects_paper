	Journal = {J Bacteriol},
	Keywords = {Agrobacterium tumefaciens,Agrobacterium tumefaciens: genetics,Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: physiology,Bacterial: genetics,Codon,Codon: genetics,DNA,Genes,Genetic,Genome,Plant,Plants,Plants: genetics,Promoter Regions,Restriction Mapping,Rhizobium,Rhizobium: genetics,Rhizobium: pathogenicity,Single-Stranded,Single-Stranded: genetics,Transformation,Virulence,Virulence: genetics},
