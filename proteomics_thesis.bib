	Keywords = {Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial Proteins: metabolism,Bacterial: genetics,Binding Sites,Biological,Bradyrhizobium,Bradyrhizobium: genetics,Bradyrhizobium: physiology,DNA,Gene Expression Profiling,Gene Expression Regulation,Genetic,Hemeproteins,Hemeproteins: genetics,Hemeproteins: metabolism,Models,Nitrogen Fixation,Promoter Regions,Regulon,Transcription},
