	Journal = {Curr Opin Microbiol},
	Keywords = {Automatic Data Processing,Bacterial,Bacterial Proteins,Bacterial Proteins: genetics,Bacterial: genetics,Databases,Genetic,Genome,Genome annotation,Genomic Library,Sequence Analysis,Sequence Analysis: methods},
