	Keywords = {Bacterial,Bacterial Proteins,Cloning,Enzymologic,Gene Expression Regulation,Genetic Complementation Test,Iron-Sulfur Proteins,Iron-Sulfur Proteins: genetics,Iron-Sulfur Proteins: metabolism,Molecular,Mutation,Nitrogen Fixation,Nitrogen Fixation: genetics,Nitrogenase,Nitrogenase: genetics,Nitrogenase: metabolism,Operon,Precipitin Tests,Rhodobacter capsulatus,Rhodobacter capsulatus: enzymology,Rhodobacter capsulatus: genetics,nitrogen fixation,regulation},
