	Author = {Chapman, John D. and Goodlett, David R. and Masselon, Christophe D.},
	Doi = {10.1002/mas.21400},
	Eprint = {NIHMS150003},
	File = {:Users/cuklinaj/Dropbox/PhD/Articles{\_}sorted/Chapman, Goodlett, Masselon - 2014 - Multiplexed and data-independent tandem mass spectrometry for global proteome profiling.pdf:pdf},
	Isbn = {0019-5189},
	Issn = {10982787},
	Journal = {Mass Spectrometry Reviews},
	Month = {nov},
	Number = {6},
	Pages = {452--470},
	Pmid = {24281846},
	Publisher = {Wiley-Blackwell},
	Title = {{Multiplexed and data-independent tandem mass spectrometry for global proteome profiling}},
