	Isbn = {1471-2288},
	Issn = {1471-2288 (Electronic) 1471-2288 (Linking)},
	Journal = {BMC Med Res Methodol},
	Pages = {13},
	Pmid = {15169550},
	Title = {{Incongruence between test statistics and P values in medical papers}},
	Type = {Journal Article},
