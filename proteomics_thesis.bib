	Issn = {1098-5336},
	Journal = {Applied and environmental microbiology},
	Month = {jul},
	Number = {19},
	Pages = {6812--6818},
	Pmid = {22798355},
	Title = {{Identification of a p-coumarate degradation regulon in Rhodopseudomonas palustris using Xpression, an integrated tool for prokaryotic RNA-seq data processing.}},
	Type = {Journal Article},
