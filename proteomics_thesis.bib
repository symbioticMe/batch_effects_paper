	Issn = {1860-7314},
	Journal = {Biotechnology journal},
	Keywords = {biomarkers,colon cancer,next-generation sequencing,personalized medicine,tumor heterogeneity},
	Month = {sep},
	Number = {9},
	Pages = {1104--14},
	Pmid = {25074435},
	Title = {{Personalized medicine approaches for colon cancer driven by genomics and systems biology: OncoTrack.}},
