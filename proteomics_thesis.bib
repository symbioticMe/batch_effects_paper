	File = {:Users/cuklinaj/Dropbox/PhD/Articles{\_}sorted/Sun et al. - 2013 - Comparative genomics of metabolic capacities of regulons controlled by cis-regulatory RNA motifs in bacteria.pdf:pdf},
	Issn = {1471-2164},
	Journal = {BMC genomics},
	Keywords = {bacteria,comparative genomics,gene function,regulon,riboswitch,rna regulatory motif},
	Month = {jan},
	Number = {1},
	Pages = {597},
