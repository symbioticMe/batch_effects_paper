	Journal = {Accounts of chemical research},
	Keywords = {Amino Acid Sequence,Amino Acid Substitution,Molecular Sequence Data,Molybdoferredoxin,Molybdoferredoxin: chemistry,Molybdoferredoxin: metabolism,Mutagenesis,Nitrogen Fixation,Nitrogenase,Nitrogenase: chemistry,Nitrogenase: metabolism,Oxidation-Reduction,Protein Conformation,Site-Directed},
	Month = {mar},
	Number = {3},
	Pages = {475--84},
	Pmid = {20030377},
	Title = {{Decoding the nitrogenase mechanism: the homologue approach.}},
	Type = {Journal Article},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/20030377 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2840065{\&}tool=pmcentrez{\&}rendertype=abstract},
