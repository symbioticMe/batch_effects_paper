	Journal = {Molecular {\&} cellular proteomics : MCP},
	Keywords = {BXD mouse population,Gene Expression*,Mitochondria function or biology,Mouse models,Protein Translocation*,SWATH-MS,Subcellular Separation,Subcellular analysis,Tissues*,electron transport chain,multiomics,transcriptomics},
	Month = {jun},
