	Issn = {1471-2105 (Electronic) 1471-2105 (Linking)},
	Journal = {BMC Bioinformatics},
	Keywords = {Animals,Cattle,Chickens,Chromatography,Computational Biology,Computational Biology: methods,Computational Biology: statistics {\&} numerical data,Computational Biology: trends,Horses,Humans,Learning,Liquid,Liquid: methods,Liquid: statistics {\&} numerical dat,Liquid: trends,Peptides,Peptides: analysis,Peptides: classification,Protein,Protein: methods,Protein: statistics {\&} numerical,Protein: trends,Proteomics,Proteomics: methods,Proteomics: statistics {\&} numerical data,Proteomics: trends,Sequence Analysis,Tandem Mass Spectrometry,Tandem Mass Spectrometry: methods,Tandem Mass Spectrometry: statistics {\&} numerical d,Tandem Mass Spectrometry: trends},
	Month = {jan},
